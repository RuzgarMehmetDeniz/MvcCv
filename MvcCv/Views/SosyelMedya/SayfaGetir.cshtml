@model MvcCv.Models.Entity.TblSosyelMedya

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLoyout.cshtml";
}

<h2>SayfaGetir</h2>

<br />
<h4>Sosyel Medya Hesap Düzenleme Sayfası</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Sosyel Medya Adı")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Güncelle</button>
</form>

